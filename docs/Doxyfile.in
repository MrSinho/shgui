PROJECT_NAME           = "shgui"
PROJECT_BRIEF          = simple and lightweight UI and console system for Vulkan applications written completely in C
PROJECT_NUMBER         = ${SH_GUI_VERSION}
PROJECT_LOGO           = 

PROJECT_BRIEF          = "shgui by mrsinho"
USE_MDFILE_AS_MAINPAGE = ${SH_GUI_ROOT_DIR}/README.md

OUTPUT_DIRECTORY       = docs
INPUT                  = ${SH_GUI_ROOT_DIR}/shgui/include ${SH_GUI_ROOT_DIR}/README.md ${SH_GUI_ROOT_DIR}/docs/src/demo.md ${SH_GUI_ROOT_DIR}/.shci/linux/log.md ${SH_GUI_ROOT_DIR}/.shci/windows/log.md 
RECURSIVE              = YES
EXTRACT_ALL            = YES
HAVE_DOT               = NO
HIDE_UNDOC_MEMBERS     = YES
GENERATE_HTML          = YES
GENERATE_XML           = YES
GENERATE_LATEX         = YES
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
OPTIMIZE_OUTPUT_FOR_C  = YES
STRIP_CODE_COMMENTS    = NO

IMAGE_PATH             = ${SH_GUI_ROOT_DIR}/docs/media ${SH_GUI_ROOT_DIR}/.shci/linux ${SH_GUI_ROOT_DIR}/.shci/windows

HTML_COLORSTYLE        = LIGHT
HTML_EXTRA_STYLESHEET  = ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome.css ${SH_GUI_ROOT_DIR}/docs/custom.css
HTML_EXTRA_FILES       = ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-fragment-copy-button.js ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-interactive-toc.js ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-paragraph-link.js ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-tabs.js ${SH_GUI_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-darkmode-toggle.js

HTML_HEADER            = ${SH_GUI_ROOT_DIR}/docs/header.html

HTML_COLORSTYLE_HUE    = 209
HTML_COLORSTYLE_SAT    = 255
HTML_COLORSTYLE_GAMMA  = 113